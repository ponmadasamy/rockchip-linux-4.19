		pinctrl-names = "default";
		pinctrl-0 = <&touch_interrupt_gpio>;
		interrupt-parent = <&gpio1>;
		interrupts = <20 IRQ_TYPE_LEVEL_LOW>;
		gpio-line-name = "lcd_enable", 
						 "touch_reset", 
						 "logo_led", 
						 "button1", "button2", "button3", "button4", "button5";
	};


	sgtl5000: codec@0a {
		status = "okay";
		#sound-dai-cells = <0>;
		compatible = "fsl,sgtl5000";
		reg = <0x0a>;
		VDDA-supply = <&vdda_codec>;
		VDDIO-supply = <&vdda_codec>;
		VDDD-supply = <&vddd_codec>;
		clocks = <&sgtl5000_clk>;
	};

	lm75@48 {
		status = "okay";
		compatible = "lm75a";
		reg = <0x48>;
	};

	eeprom@51 {
		status = "disabled";
		compatible = "24c64";
		reg = <0x51>;
	};

	tsc2007@4a {
		status = "disabled";
		compatible = "ti,tsc2007";
		reg = <0x4a>;
		interrupt-parent = <&gpio1>;
		interrupts = <20 IRQ_TYPE_LEVEL_LOW>;
		gpios = <&gpio1 20 GPIO_ACTIVE_HIGH>;
		ti,x-plate-ohms = <180>;
	};

	touch: ilitek@41 {
		status = "disabled";
		compatible = "tchip,ilitek";
		reg = <0x41>;
		// vdd-supply = <&deserializer_pwr>;
		interrupt-parent = <&gpio1>;
		interrupts = <20 IRQ_TYPE_LEVEL_LOW>;
		ilitek,irq-gpio = <&gpio1 20 GPIO_ACTIVE_HIGH>;
		// ilitek,reset-gpio = <&gpio_expander 1 GPIO_ACTIVE_HIGH>;
		// ilitek,vbus = "vcc_i2c";
		// ilitek,vdd = "vdd";
		// ilitek,name = "ilitek_i2c";
	};

	dealcore: dealcore@01 {
		status = "okay";
		compatible = "duragon,mpu";
		reg = <0x01>;
	};
};

&rt5651_card {
	status = "disabled";
};

&i2s0 {
	status = "disabled";
